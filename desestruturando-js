  matricula: 234,
  nome: "Marcos",
  telefone: 889438932,
  cidade: "SÃ£o Paulo"
}

var aluno1 = {
  matricula: 682,
  nome: "Dina",
  telefone: 889438932,
  cidade: "Brasilia"
}

var aluno2 = {
  matricula: 999,
  nome: "Ana",
